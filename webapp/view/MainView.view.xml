 <mvc:View controllerName="com.myorg.myUI5App8.controller.MainView"
	displayBlock="true"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">
	<Shell>
	<App id="app">
		<pages>
			<Page title="{i18n>title}">
				<content>
				
				<List items="{/Authors}"> 
					<items>
						<StandardListItem
							title="{ID}"
							type="Navigation"
							description="{name}"
							press=".onListItemPressed"/>
					</items>
				</List>

				

				</content>
			</Page>
		</pages>
	</App>
	</Shell>
</mvc:View> 
